client
dev tun0
daemon
proto udp
remote 0.0.0.0 1194
nobind
persist-key
persist-tun
mute-replay-warnings
ca server/ca.crt
cert server/XXXXXX.crt
key server/XXXXXX.key
tls-auth server/tls_auth.key 1
comp-lzo
verb 4
status /var/log/openvpn-XXXXXX.status.log
log-append  /var/log/openvpn-XXXXXX.log

